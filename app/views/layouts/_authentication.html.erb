<% if current_organization %>
    Signed into <%= current_organization.name %> as <%= current_organization.email %>
    <%= link_to 'Logout', logout_path, method: :delete, class: 'btn btn-primary' %>
<% else %>
    <%= link_to 'Signup', new_organizations_path, class: 'btn btn-primary' %>
    <%= link_to 'Login', login_path, class: 'btn btn-primary' %>
<% end %>