<div class="jumbotron">
  <div class="row">
    <div class="col-xs-6 center-block">
      <%= link_to image_tag('google_signin.png'), '/auth/google_oauth2' %>
    </div>
  </div>
  <div class="row">
    <div>
      <h1>Login</h1>
      </br>
      <%= form_tag login_path do %>
          <p>
            <%= label_tag :email %>
            <%= text_field_tag :email %>
          </p>
          <p>
            <%= label_tag :password %>
            <%= password_field_tag :password %>
          </p>
          <p class="button"><%= submit_tag 'Login' %></p>
      <% end %>
    </div>
  </div>
</div>
